<head>
<meta charset="utf-8" />
<title>RedLine</title>
<script type="text/javascript" src="http://ajax.microsoft.com/ajax/jquery/jquery-1.3.2.min.js"></script>
</head>
<body>
<form id="redline">
FROM <select id="fromOptions">
</select>
</form>
<script type="text/javascript">
$(document).ready(function(){
alert('REDLINE');
$.getJSON("http://realtime.mbta.com/developer/api/v2/stopsbyroute?api_key=wX9NwuHnZU2ToO7GmGR9uw&route=931_&format=json",
function(data) {
	var direction = data.direction;
	var options =[];
	for(var i in direction){
		var stops = direction[i].stop;
		for(var j in stops){
			var stopNames = stops[j].stop_name;
			//alert(stopNames);
			options.push('<option value="',stopNames,'">',
			stopNames, '</option>');
		}
		$('#fromOptions').html(options.join(''));
}

});
});

</script>
</body>
</html>
